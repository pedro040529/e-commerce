<div{{ attributes }}
    data-action="change->live#update"
>
    {{ form_start(this.form, {
        action: path('app_register'),
    }) }}
        {{ form_row(this.form.email) }}
        {{ form_row(this.form.plainPassword, {label: 'Password'}) }}
        {{ form_row(this.form.Persona.nombres)}}
        {{ form_row(this.form.Persona.apellidoMaterno)}}
        {{ form_row(this.form.Persona.apellidoPaterno)}}
        {{ form_row(this.form.Persona.numDocumento)}}
        {{ form_row(this.form.Persona.direccion)}}
        {{ form_row(this.form.Persona.telefono)}}
        {{ form_row(this.form.agreeTerms) }}
        <button type="submit" class="btn btn-primary" formnovalidate>Register</button>
    {{ form_end(this.form) }}
</div>
